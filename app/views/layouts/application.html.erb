<!DOCTYPE html>
<html>
<head>
    <title>G360EventApplication</title>
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
</head>
<body>
        <%= render 'layouts/main_navigation' %>
    <div class="container">
        <% if notice %>
        <div class="alert alert-info alert-dismissable"><button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button><%= notice %></div>
        <% end %>
        <% if alert %>
        <div class="alert alert-warning alert-dismissable"><button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button><%= alert %></div>
        <% end %>
        <%= yield %>
    </div>
</body>
</html>
