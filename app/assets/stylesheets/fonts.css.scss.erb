// Fonts
@font-face {
    font-family: 'TrendSansOne';
    src: url('<%= asset_path('29CE32_6_0.eot') %>');
    src: url('<%= asset_path('29CE32_6_0.woff') %>') format('woff'),
         url('<%= asset_path('29CE32_6_0.ttf') %>') format('truetype');
}

@font-face {
    font-family: 'TrendSlabOne';
    src: url('<%= asset_path('29CE32_10_0.eot') %>');
    src: url('<%= asset_path('29CE32_10_0.woff') %>') format('woff'),
         url('<%= asset_path('29CE32_10_0.ttf') %>') format('truetype');
}

@font-face {
    font-family: 'Gotham Book';
    src: url('<%= asset_path('Gotham-Book.eot') %>');
    src: url('<%= asset_path('Gotham-Book.eot?#iefix') %>') format('embedded-opentype'),
        url('<%= asset_path('Gotham-Book.woff') %>') format('woff'),
        url('<%= asset_path('Gotham-Book.ttf') %>') format('truetype'),
        url('<%= asset_path('Gotham-Book.svg') %>') format('svg');
}

@font-face {
    font-family: 'Gotham';
    font-weight: normal;
    src: url('<%= asset_path('Gotham-Medium.eot') %>');
    src: url('<%= asset_path('Gotham-Medium.eot?#iefix') %>') format('embedded-opentype'),
        url('<%= asset_path('Gotham-Medium.woff') %>') format('woff'),
        url('<%= asset_path('Gotham-Medium.ttf') %>') format('truetype'),
        url('<%= asset_path('Gotham-Medium.svg') %>') format('svg');
}

@font-face {
    font-family: 'Gotham';
    font-weight: bold;
    src: url('<%= asset_path('Gotham-Bold.eot') %>');
    src: url('<%= asset_path('Gotham-Bold.eot?#iefix') %>') format('embedded-opentype'),
        url('<%= asset_path('Gotham-Bold.woff') %>') format('woff'),
        url('<%= asset_path('Gotham-Bold.ttf') %>') format('truetype'),
        url('<%= asset_path('Gotham-Bold.svg') %>') format('svg');
}
