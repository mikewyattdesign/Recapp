jQuery ->
	$("[data-tag-list]").tokenInput("/tags.json", {
		crossDomain: false
		prePopulate: $("[data-tag-list]").data 'pre'
		tokenValue: "name"
		preventDuplicates: true
		theme: "facebook"
		allowFreeTagging: true
		})

jQuery ->
    $(".tags-button").on "click", ->
        $(this).parent().prev(".tag-list").toggleClass("inactive")
        .toggleClass("active")
    $(".favorite-button").on "click", ->
        $(this).parents(".favorite-box").toggleClass("active")